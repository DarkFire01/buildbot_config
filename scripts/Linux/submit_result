#!/bin/bash
source ../../config.inc
source ../../credentials.inc

if [ "$1" = "" ]; then
	echo "Please specify the build number!"
	exit 1
fi

echo "* Transferring winetest results for build $1."
echo "* Using builder $TESTMAN_BUILDER."
TESTMAN_OUT=`wget -q -O - "http://www.reactos.org/testman/webservice/buildbot_aggregator.php?sourceid=$TESTMAN_ID&password=$TESTMAN_PASS&builder=$TESTMAN_BUILDER&platform=0&build=$1"`
echo "$TESTMAN_OUT"

if [ "$TESTMAN_OUT" != "OK" ]; then
	exit 1
fi

exit 0